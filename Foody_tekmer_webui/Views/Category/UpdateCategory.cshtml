@model FoodyTekmerEntityLayer.Concrete.Category
@{
    ViewData["Title"] = "UpdateCategory";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
<div class="container">
   
    <h3>Kategori Güncelleme Sayfası</h3>
    <form method="post">
        <input type="text" asp-for="Id"  class="form-control" hidden/>
        <input type="text" asp-for="status" class="form-control" hidden />
      
        <label>Kategori Adı : </label>
        <input type="text" asp-for="CategoryName" maxlength="50" class="form-control" />
        <br />
        <button type="submit" class="btn btn-info">Kategori Güncelle</button>
    </form>
</div>
